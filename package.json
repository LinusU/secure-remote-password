{
	"name": "secure-remote-password",
	"version": "1.0.0",
	"license": "MIT",
	"main": "es5/index.js",
	"module": "es6/index.js",
	"source": "src/index.ts",
	"scripts": {
		"es5": "tsc -p tsconfig.json --target es6 --outDir es6 --module es6 --watch",
		"es6": "tsc -p tsconfig.json --target es5 --outDir es5 --module commonjs --watch",
		"tsc": "concurrently \"npm run es5\" \"npm run es6\"",
		"watch": "concurrently \"npm run es5\" \"npm run es6\""
	},
	"dependencies": {
		"@types/node": "^15.6.0",
		"array-buffer-to-hex": "^1.0.0",
		"crypto-digest-sync": "^1.0.0",
		"crypto-random-hex": "^1.0.0",
		"encode-utf8": "^1.0.3",
		"fast-sha256": "^1.3.0",
		"hex-to-array-buffer": "^1.1.0",
		"jsbn": "^1.1.0",
		"pad-start": "^1.0.2"
	},
	"devDependencies": {
		"@types/jsbn": "^1.2.29",
		"concurrently": "^6.1.0",
		"mocha": "^8.4.0",
		"standard": "^16.0.3",
		"typescript": "^4.2.4"
	}
}
